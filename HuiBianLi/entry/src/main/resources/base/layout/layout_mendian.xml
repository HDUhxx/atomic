<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical">
    <DirectionalLayout
        ohos:height="100px"
        ohos:width="match_parent">
        <Text
            ohos:height="match_parent"
            ohos:width="match_parent"
            ohos:text="服务门店:"
            ohos:text_size="28fp"/>
    </DirectionalLayout>
    <Component
        ohos:height="40px"
        ohos:width="match_parent"/>
    <DirectionalLayout
        ohos:height="1600px"
        ohos:width="match_parent"
        ohos:orientation="vertical">
        <ScrollView
            ohos:height="match_parent"
            ohos:width="match_parent">
            <DirectionalLayout
                ohos:height="match_content"
                ohos:width="match_parent"
                ohos:orientation="vertical">
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical">
                    <Button
                        ohos:height="100px"
                        ohos:width="match_parent"
                        ohos:text="亿嘉超市"
                        ohos:text_size="18fp"
                        ohos:text_alignment="left"/>
                    <Image
                        ohos:height="match_content"
                        ohos:width="match_parent"
                        ohos:image_src="$media:daohua"/>
                </DirectionalLayout>
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical">
                    <Button
                        ohos:height="100px"
                        ohos:width="match_parent"
                        ohos:text="万源超市"
                        ohos:text_size="18fp"
                        ohos:text_alignment="left"/>
                    <Image
                        ohos:height="match_content"
                        ohos:width="match_parent"
                        ohos:image_src="$media:wanyuan"/>
                </DirectionalLayout>
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical">
                    <Button
                        ohos:height="100px"
                        ohos:width="match_parent"
                        ohos:text="福顺达超市"
                        ohos:text_size="18fp"
                        ohos:text_alignment="left"/>
                    <Image
                        ohos:height="match_content"
                        ohos:width="match_parent"
                        ohos:image_src="$media:fusun"/>
                </DirectionalLayout>
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical">
                    <Button
                        ohos:height="100px"
                        ohos:width="match_parent"
                        ohos:text="福佑超市"
                        ohos:text_size="18fp"
                        ohos:text_alignment="left"/>
                    <Image
                        ohos:height="match_content"
                        ohos:width="match_parent"
                        ohos:image_src="$media:fuyou"/>
                </DirectionalLayout>
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical">
                    <Button
                        ohos:height="100px"
                        ohos:width="match_parent"
                        ohos:text="好财福百货超市"
                        ohos:text_size="18fp"
                        ohos:text_alignment="left"/>
                    <Image
                        ohos:height="match_content"
                        ohos:width="match_parent"
                        ohos:image_src="$media:haocai"/>
                </DirectionalLayout>
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical">
                    <Button
                        ohos:height="100px"
                        ohos:width="match_parent"
                        ohos:text="壹仟家便利店"
                        ohos:text_size="18fp"
                        ohos:text_alignment="left"/>
                    <Image
                        ohos:height="match_content"
                        ohos:width="match_parent"
                        ohos:image_src="$media:yijia"/>
                </DirectionalLayout>
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical">
                    <Button
                        ohos:height="100px"
                        ohos:width="match_parent"
                        ohos:text="馨安便利店"
                        ohos:text_size="18fp"
                        ohos:text_alignment="left"/>
                    <Image
                        ohos:height="match_content"
                        ohos:width="match_parent"
                        ohos:image_src="$media:xinan"/>
                </DirectionalLayout>
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical">
                    <Button
                        ohos:height="100px"
                        ohos:width="match_parent"
                        ohos:text="轫益便利店"
                        ohos:text_size="18fp"
                        ohos:text_alignment="left"/>
                    <Image
                        ohos:height="match_content"
                        ohos:width="match_parent"
                        ohos:image_src="$media:renyi"/>
                </DirectionalLayout>
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical">
                    <Button
                        ohos:height="100px"
                        ohos:width="match_parent"
                        ohos:text="千年喜便利店"
                        ohos:text_size="18fp"
                        ohos:text_alignment="left"/>
                    <Image
                        ohos:height="match_content"
                        ohos:width="match_parent"
                        ohos:image_src="$media:qiannian"/>
                </DirectionalLayout>
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical">
                    <Button
                        ohos:height="100px"
                        ohos:width="match_parent"
                        ohos:text="利客便利店"
                        ohos:text_size="18fp"
                        ohos:text_alignment="left"/>
                    <Image
                        ohos:height="match_content"
                        ohos:width="match_parent"
                        ohos:image_src="$media:like"/>
                </DirectionalLayout>
            </DirectionalLayout>
        </ScrollView>
    </DirectionalLayout>

</DirectionalLayout>