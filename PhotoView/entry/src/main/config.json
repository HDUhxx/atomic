{
  "app": {
    "bundleName": "com.github.chrisbanes.sample",
    "vendor": "github",
    "version": {
      "code": 1,
      "name": "1.0"
    },
    "apiVersion": {
      "compatible": 5,
      "target": 5
    }
  },
  "deviceConfig": {},
  "module": {
    "reqPermissions": [
      {
        "usedScene": {
          "when": "always",
          "ability": [
            "com.github.chrisbanes.sample.MainAbility"
          ]
        },
        "reason": "use internet",
        "name": "ohos.permission.INTERNET"
      }
    ],
    "package": "com.github.chrisbanes.sample",
    "name": ".MyApplication",
    "reqCapabilities": [
      "video_support"
    ],
    "deviceType": [
      "phone",
      "tv"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",

        "name": "com.github.chrisbanes.sample.TestAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",

        "name": "com.github.chrisbanes.sample.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",

        "name": "com.github.chrisbanes.sample.PageAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "metaData": {
      "customizeData": [
        {
          "name": "hwc-theme",
          "value": "androidhwext:style/Theme.Emui.NoTitleBar",
          "extra": ""
        }]
    }
  }
}