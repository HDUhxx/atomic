            {
                "name": "ohos.permission.DISTRIBUTED_DATASYNC"
            },
            {
                "name": "ohos.permission.DISTRIBUTED_DEVICE_STATE_CHANGE"
            },
            {
                "name": "ohos.permission.GET_DISTRIBUTED_DEVICE_INFO"
            },
            {
                "name": "ohos.permission.SYSTEM_FLOAT_WINDOW"
