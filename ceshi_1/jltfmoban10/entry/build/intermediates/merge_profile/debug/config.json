{
    "app": {
        "bundleName": "com.example.jltfmoban10",
        "vendor": "example",
        "apiVersion": {
            "compatible": 5,
            "target": 5,
            "releaseType": "Beta1"
        },
        "version": {
            "code": 1,
            "name": "1.0"
        }
    },
    "deviceConfig": {
        
    },
