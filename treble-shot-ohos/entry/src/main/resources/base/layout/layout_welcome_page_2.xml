<?xml version="1.0" encoding="utf-8"?>
<DependentLayout xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:width="match_parent"
    ohos:height="match_parent">

    <include
        ohos:id="$+id:mainContainer"
        ohos:layout="$layout:header_default_device_os"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:center_in_parent="true"/>

    <Text
        ohos:width="match_parent"
        ohos:height="match_content"
        ohos:above="$id:mainContainer"
        ohos:text_alignment="center"
        ohos:align_parent_top="true"
        ohos:alignment="center"
        ohos:text="$string:text_introSetUpProfile"
        ohos:text_size="$float:font_size_title" />

</DependentLayout>