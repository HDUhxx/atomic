{
    "app": {
        "bundleName": "com.huawei.cookbook",
        "vendor": "huawei",
        "apiVersion": {
            "compatible": 7,
            "target": 7,
            "releaseType": "Beta2"
        },
        "version": {
            "code": 1000000,
            "name": "1.0.0"
        }
    },
    "deviceConfig": {
        
    },
    "module": {
        "name": ".MyApplication",
        "deviceType": [
            "phone",
            "tablet"
        ],
        "distro": {
            "deliveryWithInstall": true,
            "moduleName": "entry",
            "moduleType": "entry",
            "installationFree": true
        },
        "abilities": [
            {
                "name": "com.huawei.cookbook.MainAbility",
                "description": "$string:description",
                "icon": "$media:icon",
                "label": "$string:app_name",
                "type": "page",
                "launchType": "standard",
                "orientation": "portrait",
                "skills": [
                    {
                        "actions": [
                            "action.system.home"
                        ],
                        "entities": [
                            "entity.system.home"
                        ]
                    }
                ],
                "homeAbility": true
            },
            {
                "name": "com.huawei.cookbook.ScanAbility",
                "description": "$string:description",
                "icon": "$media:icon",
                "label": "$string:app_name",
                "type": "page",
                "launchType": "standard",
                "orientation": "portrait",
                "homeAbility": false
            }
        ],
        "reqPermissions": [
            {
                "name": "ohos.permission.CAMERA"
            }
        ],
        "package": "com.huawei.cookbook"
    }
}