<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:alignment="center"
    ohos:orientation="vertical">

    <TextField
        ohos:id="$+id:text"
        ohos:height="100px"
        ohos:text_size="50px"
        ohos:width="500px"
        ohos:background_element="#FFBFDDDC"
        />

    <Button
        ohos:id="$+id:Nabtn"
        ohos:height="200px"
        ohos:width="500px"
        ohos:text="打开地图"
        ohos:text_size="50px"
        ohos:background_element="#FF584BCF"
        />


</DirectionalLayout>