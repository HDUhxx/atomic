<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
   >

    <ScrollView
        ohos:height="match_parent"
        ohos:width="match_parent"
        >

        <DirectionalLayout
            ohos:height="match_content"
            ohos:width="match_parent"
            ohos:orientation="vertical"
            >

            <Button
                ohos:id="$+id:open_browser"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:padding="10vp"
                ohos:background_element="$graphic:background_ability_main"
                ohos:layout_alignment="left"
                ohos:text="$string:openbrower"
                ohos:text_size="50"
                ohos:top_margin="20vp"
                ohos:start_margin="20vp"
                ohos:end_margin="20vp"
                />
            <Button
                ohos:id="$+id:toPrinterActivityWithRequestLogin"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:background_element="$graphic:background_ability_main"
                ohos:layout_alignment="left"
                ohos:text="跳转到数据打印页(使用动态登录检查)"
                ohos:top_margin="10vp"
                ohos:text_size="50"
                ohos:padding="10vp"
                ohos:start_margin="20vp"
                />
            <Button
                ohos:id="$+id:toPrinterActivityWithInterceptor"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:background_element="$graphic:background_ability_main"
                ohos:layout_alignment="left"
                ohos:text="跳转到数据打印页(使用指定登录拦截器"
                ohos:top_margin="10vp"
                ohos:text_size="50"
                ohos:padding="10vp"
                ohos:start_margin="20vp"
                />
            <Button
                ohos:id="$+id:toPrinterActivityWithoutInterceptor"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:background_element="$graphic:background_ability_main"
                ohos:layout_alignment="left"
                ohos:text="跳转到数据打印页(不触发拦截器)"
                ohos:top_margin="10vp"
                ohos:text_size="50"
                ohos:padding="10vp"
                ohos:start_margin="20vp"
                />
            <Button
                ohos:id="$+id:toUserActivity"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:background_element="$graphic:background_ability_main"
                ohos:layout_alignment="left"
                ohos:text="跳转到用户信息页(自带登录拦截器)"
                ohos:top_margin="10vp"
                ohos:text_size="50"
                ohos:padding="10vp"
                ohos:start_margin="20vp"
                />
            <Button
                ohos:id="$+id:toPrinterActivityWithExtras"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:background_element="$graphic:background_ability_main"
                ohos:layout_alignment="left"
                ohos:text="跳转到数据打印页(添加额外数据)"
                ohos:top_margin="10vp"
                ohos:text_size="50"
                ohos:padding="10vp"
                ohos:start_margin="20vp"
                />
            <Button
                ohos:id="$+id:toResultActivity"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:background_element="$graphic:background_ability_main"
                ohos:layout_alignment="left"
                ohos:text="使用ActivityResult回调"
                ohos:top_margin="10vp"
                ohos:text_size="50"
                ohos:padding="10vp"
                ohos:start_margin="20vp"
                />
            <Button
                ohos:id="$+id:toAutoParseActivity"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:background_element="$graphic:background_ability_main"
                ohos:layout_alignment="left"
                ohos:text="跳转到Arg参数自动解析页"
                ohos:top_margin="10vp"
                ohos:text_size="50"
                ohos:padding="10vp"
                ohos:start_margin="20vp"
                />
            <Button
                ohos:id="$+id:launchActionRoute"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:background_element="$graphic:background_ability_main"
                ohos:layout_alignment="left"
                ohos:text="启动一个简单的动作路由"
                ohos:top_margin="10vp"
                ohos:text_size="50"
                ohos:padding="10vp"
                ohos:start_margin="20vp"
                />
            <Button
                ohos:id="$+id:launchActionRouteWithExecutorAnnotation"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:background_element="$graphic:background_ability_main"
                ohos:text="启动一个简单的动作路由(使用注解进行线程指定)"
                ohos:top_margin="10vp"
                ohos:text_size="50"
                ohos:padding="10vp"
                ohos:start_margin="20vp"
                ohos:max_text_lines="2"
                ohos:end_margin="20vp"
                ohos:text_alignment="left"
                ohos:multiple_lines="true"
                ohos:bottom_margin="5vp"
                />
            <Button
                ohos:id="$+id:launchActionRouteWithExecutorConfig"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:background_element="$graphic:background_ability_main"
                ohos:text_alignment="left"
                ohos:text="启动一个简单的动作路由(使用执行器进行线程指定)"
                ohos:top_margin="10vp"
                ohos:text_size="50"
                ohos:padding="10vp"
                ohos:start_margin="20vp"
                ohos:end_margin="20vp"
                ohos:multiple_lines="true"
                ohos:bottom_margin="5vp"
                />
            <Button
                ohos:id="$+id:createInstanceForJavaBean"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:background_element="$graphic:background_ability_main"
                ohos:text="使用对象路由 创建实体bean"
                ohos:top_margin="10vp"
                ohos:text_size="50"
                ohos:padding="10vp"
                ohos:start_margin="20vp"
                />
        </DirectionalLayout>

    </ScrollView>


</DirectionalLayout>